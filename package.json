{
  "name": "@kalo/ui",
  "version": "0.10.2",
  "description": "A library of React components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalohq/ui"
  },
  "main": "lib/components/index.js",
  "files": ["lib"],
  "scripts": {
    "start": "npm run storybook:start",
    "storybook": "npm run storybook:start",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o docs",
    "compile":
      "npm run generate-tokens && npm run generate-icon-symbols && gulp build-production",
    "test": "jest",
    "dist": "rimraf lib && npm run compile",
    "copy-flow":
      "flow-copy-source src/components lib --ignore '{index.js,**/__tests__/*,**/__stories__/*}'",
    "typecheck": "flow",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "generate-tokens": "node scripts/tokens.js",
    "generate-icon-symbols": "node scripts/icons.js"
  },
  "lint-staged": {
    "*.js": ["eslint"]
  },
  "author": "The Kalo Team",
  "license": "ISC",
  "devDependencies": {
    "@kalo/eslint-config": "6.0.0",
    "@storybook/addon-info": "3.0.0",
    "@storybook/addon-options": "3.0.0",
    "@storybook/cli": "3.0.0",
    "@storybook/react": "3.0.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-istanbul": "0.8.0",
    "babel-jest": "21.0.2",
    "babel-loader": "7.0.0",
    "babel-plugin-emotion": "^8.0.10",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.0",
    "emotion": "^8.0.10",
    "emotion-theming": "^8.0.10",
    "enzyme": "2.7.1",
    "enzyme-to-json": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-prettier": "2.4.0",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "2.0.1",
    "eslint-plugin-react": "7.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "flow-bin": "0.53.0",
    "flow-copy-source": "1.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-postcss": "^7.0.0",
    "husky": "0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "21.0.0",
    "jest-glamor-react": "3.0.0",
    "lint-staged": "3.4.2",
    "lodash": "4.17.2",
    "postcss-cli": "3.2.0",
    "postcss-css-variables": "0.7.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "prettier": "1.6.1",
    "prop-types": "15.5.10",
    "qs": "5.2.1",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-emotion": "^8.0.10",
    "react-hotkeys": "0.9.0",
    "react-intl": "2.1.5",
    "react-pure-render": "1.0.2",
    "react-storybook-addon-add-stories-group": "0.1.0",
    "react-test-renderer": "15.4.1",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "styled-system": "^1.0.8",
    "svg-sprite": "^1.3.7",
    "theo": "^6.0.0-beta.9",
    "webpack": "2.5.1"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "prop-types": "^15.5.10",
    "qs": "^5.2.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hotkeys": "^0.9.0",
    "react-intl": "^2.1.5",
    "react-pure-render": "^1.0.2"
  },
  "babel": {
    "presets": ["es2015", "stage-2", "react"],
    "ignore": ["__stories__"]
  },
  "jest": {
    "modulePaths": ["src/core", "src"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$":
        "<rootDir>/src/utils/test/mocks/file-mock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "testMatch": ["**/__tests__/**/?(*.)(spec|test).js?(x)"],
    "coverageDirectory": "./coverage/unit",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.stories.js"
    ]
  },
  "dependencies": {}
}
