{
  "name": "@kalo/ui",
  "version": "3.10.0",
  "description": "A library of React components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalohq/ui"
  },
  "main": "lib/components/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && npm run compile",
    "compile": "npm run generate:tokens && npm run generate:icons && gulp build-npm-package",
    "test": "jest",
    "release": "np --no-yarn --no-cleanup --any-branch",
    "typecheck": "flow",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "generate:tokens": "node scripts/tokens.js",
    "generate:icons": "node scripts/icons.js",
    "version": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "author": "The Kalo Team",
  "license": "ISC",
  "devDependencies": {
    "@kalo/eslint-config": "6.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "22.4.1",
    "babel-loader": "7.0.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "flow-bin": "0.53.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^7.0.0",
    "husky": "0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "22.4.1",
    "lint-staged": "3.4.2",
    "lodash": "^4.17.11",
    "np": "^3.1.0",
    "polished": "^2.3.3",
    "postcss-cli": "3.2.0",
    "postcss-css-variables": "0.7.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "9.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.16.3",
    "prop-types": "^15.5.10",
    "qs": "5.2.1",
    "react": "15.6.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "15.6.2",
    "react-hotkeys": "^0.9.0",
    "react-intl": "2.1.5",
    "react-test-renderer": "15.6.2",
    "recompose": "0.20.2",
    "rimraf": "2.6.1",
    "sinon": "^7.2.6",
    "style-loader": "0.16.1",
    "svg-sprite": "^1.5.0",
    "theo": "^8.1.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "prop-types": "^15.5.10",
    "qs": "^5.2.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hotkeys": "^0.9.0",
    "react-intl": "^2.1.5",
    "react": "^15.4.1",
    "recompose": "0.20.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "ignore": [
      "__examples__"
    ]
  },
  "jest": {
    "modulePaths": [
      "src/core",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/src/utils/test/mocks/file-mock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "config"
    ],
    "setupFiles": [
      "./src/utils/test/global-context.js"
    ],
    "coverageDirectory": "./coverage/unit",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/index.js",
      "!src/**/*.examples.js",
      "!src/components/icon-symbols/**/*",
      "!src/design-tokens/**/*",
      "!src/icons/**/*"
    ]
  },
  "dependencies": {}
}
